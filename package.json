{
  "name": "as-external-uri",
  	"publisher": "jsjoeio",
	"displayName": "as-external-uri",
	"description": "",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/jsjoeio/as-external-uri-test"
	},
	"engines": {
		"vscode": "^1.71.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onStartupFinished"
	],
	"main": "./out/extension.js",
	"browser": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "as-external-uri.askTestCase",
				"title": "As External URI: input test case"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"vscode:package": "npx vsce package && cp as-external-uri-*.vsix packaged/",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.71.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
